<?xml version="1.0" encoding="UTF-8"?>
<configuration status="TRACE">
  <appenders>
    <File name="File" fileName="go2.pt.log">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36}:%L - %msg%n"/>
    </File>
  </appenders>
  <loggers>
    <root level="trace">
      <appender-ref ref="File"/>
    </root>
  </loggers>
</configuration>